<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="enigma-optima.benchmark" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug x64">
				<Option output="bin/Debug/x64/enigma-optima-benchmark" prefix_auto="1" extension_auto="1" />
				<Option working_dir="bin/Debug/x64/" />
				<Option object_output="obj/Debug/x64/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-m64" />
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add option="-m64" />
					<Add directory="$(#gbench.lib)" />
					<Add directory="../libEnigma/bin/Debug/x64" />
				</Linker>
			</Target>
			<Target title="Release x64">
				<Option output="bin/Release/x64/enigma-optima-benchmark" prefix_auto="1" extension_auto="1" />
				<Option working_dir="bin/Release/x64/" />
				<Option object_output="obj/Release/x64/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O3" />
					<Add option="-m64" />
				</Compiler>
				<Linker>
					<Add option="-O3" />
					<Add option="-m64" />
					<Add directory="$(#gbench.lib)" />
					<Add directory="../libEnigma/bin/Release/x64" />
				</Linker>
			</Target>
			<Target title="Release x86">
				<Option output="bin/Release/x86/enigma-optima-benchmark" prefix_auto="1" extension_auto="1" />
				<Option working_dir="bin/Release/x86/" />
				<Option object_output="obj/Release/x86/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O3" />
					<Add option="-m32" />
				</Compiler>
				<Linker>
					<Add option="-O3" />
					<Add option="-m32" />
					<Add directory="$(#gbench.lib32)" />
					<Add directory="../libEnigma/bin/Release/x86" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wextra" />
			<Add option="-Wall" />
			<Add option="-g" />
			<Add option="-std=gnu99" />
			<Add option="-std=gnu++11" />
			<Add option="-Werror=implicit-function-declaration" />
			<Add option="-Wunsafe-loop-optimizations" />
			<Add option="-Wvector-operation-performance" />
			<Add option="-fexceptions" />
			<Add directory="$(#gbench.include)" />
			<Add directory="../../libEnigma/include" />
		</Compiler>
		<Linker>
			<Add option="-static" />
			<Add library="benchmark" />
			<Add library="Enigma" />
			<Add library="shlwapi" />
		</Linker>
		<Unit filename="../../benchmark/main.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
