				</Linker>
			</Target>
			<Target title="Release Windows x64">
				<Option output="bin/ReleaseWin64/enigma-optima-tests" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/ReleaseWin64/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-m64" />
					<Add option="-march=x86-64" />
				</Compiler>
				<Linker>
					<Add option="-m64" />
					<Add directory="../code_blocks/libEnigma/bin/Release/x64" />
					<Add directory="$(#gtest.lib)" />
				</Linker>
			</Target>
			<Target title="Release Windows x86">
				<Option output="bin/ReleaseWin32/enigma-optima-tests" prefix_auto="1" extension_auto="1" />
