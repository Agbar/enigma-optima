<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="enigma-optima.tests" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/enigma-optima" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release Windows x64">
				<Option output="bin/ReleaseWin64/enigma-optima" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/ReleaseWin64/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="../code_blocks/obj/ReleaseWin64/OS/OsWindows.o" />
					<Add library="../code_blocks/obj/ReleaseWin64/error.o" />
					<Add library="../code_blocks/obj/ReleaseWin64/date.o" />
				</Linker>
				<Environment>
					<Variable name="gtest_lib" value="$(#gtest.lib)" />
				</Environment>
			</Target>
			<Target title="Release Windows x86">
				<Option output="bin/ReleaseWin32/enigma-optima" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/ReleaseWin32/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-m32" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-m32" />
					<Add library="../code_blocks/obj/ReleaseWin32/OS/OsWindows.o" />
					<Add library="../code_blocks/obj/ReleaseWin32/error.o" />
					<Add library="../code_blocks/obj/ReleaseWin32/date.o" />
				</Linker>
				<Environment>
					<Variable name="gtest_lib" value="$(#gtest)lib32" />
				</Environment>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-std=c++11" />
			<Add option="-m64" />
			<Add option="-fexceptions" />
			<Add directory="$(#gtest.include)" />
			<Add directory="../libEnigma/include" />
		</Compiler>
		<Linker>
			<Add option="-m64" />
			<Add library="libgtest" />
			<Add directory="$(gtest_lib)" />
		</Linker>
		<Unit filename="GetRSeed.tests.cpp" />
		<Unit filename="cipher.tests.cpp" />
		<Unit filename="main.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
