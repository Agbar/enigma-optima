		</Build>
		<Compiler>
			<Add option="-Wextra" />
			<Add option="-Wall" />
			<Add option="-std=c++11" />
			<Add option="-g" />
			<Add option="-fexceptions" />
			<Add option="-pthread" />
			<Add option="-mtune=generic" />
